{
    "name": "codesleeve/stapler",
    "description": "Elegant and simple ORM-based file upload package for php.",
    "keywords": ["ORM", "file", "upload", "S3", "AWS", "paperclip"],
    "license": "MIT",
    "authors": [
        {
            "name": "Travis Bennett",
            "email": "tandrewbennett@hotmail.com"
        }
    ],
    "require": {
        "php": ">=5.6",
        "symfony/http-foundation": "~2.3|~3.0",
        "imagine/imagine": "~0.6.2",
        "doctrine/inflector": "~1"
    },
    "require-dev": {
        "mockery/mockery": "0.8.0",
        "illuminate/database": "4.*|5.*",
        "illuminate/config": "4.*|5.*",
        "league/flysystem": "^1.0",
        "league/flysystem-aws-s3-v3": "^1.0",
        "league/flysystem-rackspace": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "Codesleeve\\Stapler\\": "src/"
        }
    },
    "suggest": {
        "league/flysystem": "Required to use the AWS, Rackspace Cloud, and Azure Blob storage drivers (~1.0).",
        "league/flysystem-aws-s3-v3": "Required to use the Flysystem S3 driver (~1.0).",
        "league/flysystem-rackspace": "Required to use the Flysystem Rackspace driver (~1.0)."
    },
    "minimum-stability": "dev"
}
